/*============================================= H E A D E R ================================================*/
header {
    padding: 20px 0;
}
.logo img {
    width: 100%;
}
.search_top {
    margin-top: 8px;
    position: relative;
}
.div_search {
    width: 100%;
    border: 1px solid #d92726;
    height: 36px;
    padding: 1px;
}
.div_search input[type="text"] {
    border: none;
    height: 100%;
    width: 73%;
    padding-left: 15px;
    font-size: 13px;
    outline: none;
}
.btn_search {
    position: absolute;
    top: 0px;
    right: 16px;
    display: inline-block;
    width: 26%;
    background-color: #d92727;
    height: 36px;
    text-align: center;
    color: #fff;
}
.btn_search input[type="submit"] {
    border: none;
    background-color: #d92727;
    font-size: 15px;
    font-weight: bold;
    text-transform: uppercase;
    padding-top: 8px;
}
.btn_search:after {
    right: 100%;
    top: 50%;
    border: solid transparent;
    content: " ";
    height: 0;
    width: 0;
    position: absolute;
    pointer-events: none;
    border-color: rgba(217, 39, 39, 0);
    border-right-color: #d92727;
    border-width: 7px;
    margin-top: -7px;
}
.btn_search i {
    font-size: 18px;
}
.keysearch p {
    font-size: 12px;
    padding-top: 10px;
    font-weight: bold;
    color: #d92727;
}
.keysearch p span {
    font-weight: 100;
    color: #666;
}
.cart_header {
    padding-top: 20px;
}
.cart_header .icon_cart {
    font-size: 25px;
    position: relative;
    color: #333;
}
.cart_header span {
    position: absolute;
    top: -7px;
    right: -17px;
    font-size: 19px;
    padding: 1px 6px;
    border: 2px solid #fff;
    background-color: #d92727;
    border-radius: 50%;
    color: #fff;
    font-family: serif;
}
.cart_header h5 {
    margin: 0;
    display: inline-block;
    padding-left: 20px;
    font-size: 14px;
    text-transform: uppercase;
}
.cart_header a {
    color: #000;
}

/*=============================================== M E N U ================================================*/
