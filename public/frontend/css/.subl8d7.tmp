/*======================================= SẢN PHẨM CHI TIẾT ========================================*/
.product_detail {
    padding: 10px 0;
    background-color: #fff;
}
.product_img {
    overflow: hidden;
    background-color: #fff;
    width: 100%;
    height: 360px;
    border: 1px solid #ccc;
    margin-bottom: 15px;
    text-align: center;
}
.product_img img {
    height: 350px;
    display: inline-block;
}
.list_img_small{
    width: 100%;
    position: relative;
    display: inline-block;
}
.list_img_small:hover .product_detail_nleft, .list_img_small:hover .product_detail_nright{
    display: block;
}
.product_detail_nleft{
    position: absolute;
    top: 34%;
    left: 0;
    z-index: 1;
    padding: 5px;
    border: 1px solid #ff3333;
    opacity: 0.8;
    color: #ff3333;
    cursor: pointer;
    display: none;
}
.product_detail_nright{
    position: absolute;
    top: 34%;
    z-index: 1;
    right: 0;
    padding: 5px;
    border: 1px solid #ff3333;
    opacity: 0.8;
    color: #ff3333;
    cursor: pointer;
    display: none;
}
.product_detail_nright:hover, .product_detail_nleft:hover{
    background-color: #ff3333;
    color: #fff;
}
.list_img_small_item {
    width: 96%;
    height: 79px;
    border: 1px solid #ccc;
